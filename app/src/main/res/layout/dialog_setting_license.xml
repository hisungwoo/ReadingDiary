<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/dialog_message_background_round"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/dialog_license_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="KaKao Open SDK"
        android:gravity="center"
        android:layout_marginTop="35dp"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/dialog_license_url"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="https://developers.kakao.com/docs"
        android:textSize="15sp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/dialog_license_title_tv"
        app:layout_constraintStart_toStartOf="@+id/dialog_license_title_tv"
        app:layout_constraintEnd_toEndOf="@+id/dialog_license_title_tv" />

    <TextView
        android:id="@+id/dialog_license_url2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="https://1sam1.tistory.com/50?category=1052040"
        android:gravity="center"
        android:textSize="15sp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/dialog_license_url"
        app:layout_constraintStart_toStartOf="@+id/dialog_license_title_tv"
        app:layout_constraintEnd_toEndOf="@+id/dialog_license_title_tv" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/dialog_license_close_btn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button_background_round_gray"
        android:text="닫기"
        android:textSize="16sp"
        android:textColor="#4E4E4E"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintTop_toBottomOf="@+id/dialog_license_url2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />





</androidx.constraintlayout.widget.ConstraintLayout>